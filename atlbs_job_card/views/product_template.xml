<odoo>
<record id="product_template_vehicle_info_page" model="ir.ui.view">
    <field name="name">product.template.vehicle.info</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="arch" type="xml">

        <xpath expr="//field[@name='categ_id']" position="after">
            <field name="menu_service"/>
            <field name="service_amount"/>
        </xpath>


<!--         <xpath expr="//div[hasclass('oe_title')]" position="inside">-->
<!--                <field name="part_no" placeholder="e.g. Part No"/>-->
<!--            </xpath>-->


        <xpath expr="//div[hasclass('oe_title')]" position="inside">
                <div class="o_form_label o_td_label">
                    <label for="part_no"/>
                </div>
                <div>
                    <field name="part_no"/>
                </div>
            </xpath>






        <xpath expr="//field[@name='default_code']" position="attributes">
            <attribute name="string">Internal Reference</attribute>
        </xpath>


    </field>
</record>

</odoo>






